! Xft settings ---------------------------------------------------------------

!Xft.dpi:        85
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintsmedium
URxvt*iconFile: /usr/share/icons/Faenza/apps/scalable/terminal.svg

!-------------------------------------------------------------------------------
!
!	U R X V T - Settings
!
URxvt*geometry:					80x25
URxvt*foreground:				grey80
URxvt*borderLess:				false
URxvt*depth:					32
URxvt*background:				rgba:0000/0000/0000/dddd
!URxvt*background:				#676767
URxvt*scrollBar:			    off
!URxvt*font:						xft:Consolas:size=12
URxvt*font:                     xft:Terminus:size=10


URxvt*cursorBlink: true
URxvt*cursorUnderline: true
!transparency
URxvt*transparent: true
URxvt*shading: 10 # 0 to 99 darkens, 101 to 200 lightens

!plugins
URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      /usr/bin/opera
URxvt.matcher.button:   1

!colors
!black
URxvt*color0:					#676767
URxvt*color8:					#757575
!red
URxvt*color1:					#EA6868
URxvt*color9:					#FF7272
!green
URxvt*color2:					#ABCB8D
URxvt*color10:					#AFD78A
!yellow
URxvt*color3:					#E8AE5B
URxvt*color11:					#FFA75D
!blue
URxvt*color4:					#71C5F4
URxvt*color12:					#67CDE9
!magenta
URxvt*color5:					#E2BAF1
URxvt*color13:					#ECAEE9
!cyan
URxvt*color6:					#676767
URxvt*color14:					#757575
!white
URxvt*color7:					#F1F1F1
URxvt*color15:					#FFFFFF

XTerm*termName: xterm-256color
urxvt*termName: rxvt-256color

